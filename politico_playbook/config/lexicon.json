    "terms": {
      "whip": {
        "term": "Whip",
        "political_definition": "A member of a political party in a legislature who is responsible for ensuring party discipline and attendance for voting",
        "context_patterns": ["party whip", "majority whip", "minority whip", "house whip", "senate whip"],
        "examples": ["Kevin McCarthy served as House Majority Whip", "James Clyburn is the House Majority Whip"]
      },
      "caucus": {
        "term": "Caucus",
        "political_definition": "A meeting of members of a political party or subgroup to coordinate policy or select candidates",
        "context_patterns": ["democratic caucus", "republican caucus", "congressional caucus", "caucus chair"],
        "examples": ["The Black Caucus met on Tuesday", "Members of the Progressive Caucus opposed the bill"]
      },
      "ranking member": {
        "term": "Ranking Member",
        "political_definition": "The most senior member of the minority party on a committee",
        "context_patterns": ["ranking member of", "committee's ranking member", "ranking republican", "ranking democrat"],
        "examples": ["Jane Smith is the Ranking Member of the Judiciary Committee"]
      },
      "markup": {
        "term": "Markup",
        "political_definition": "A committee meeting to debate, amend, and rewrite proposed legislation",
        "context_patterns": ["committee markup", "markup session", "during the markup"],
        "examples": ["The bill passed out of markup with several amendments"]
      },
      "filibuster": {
        "term": "Filibuster",
        "political_definition": "A parliamentary procedure where debate is extended to delay or prevent a vote on a bill",
        "context_patterns": ["senate filibuster", "filibuster reform", "filibuster rules", "threatening a filibuster"],
        "examples": ["The senator's filibuster lasted over 12 hours"]
      }
    },
