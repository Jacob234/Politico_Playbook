        "context_patterns": ["democratic caucus", "republican caucus", "congressional caucus", "caucus chair"],
        "examples": ["The Black Caucus met on Tuesday", "Members of the Progressive Caucus opposed the bill"]
      },
      "ranking member": {
        "term": "Ranking Member",
